# Example Environment Configuration (SANITIZED)
# Copy to .env locally. NEVER commit the filled .env

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=__BASE58_PRIVATE_KEY__
SOLANA_PUBLIC_KEY=__PUBLIC_KEY__

# Trading Configuration
MAX_POSITION_SIZE=100
RISK_PERCENTAGE=5
STOP_LOSS_PERCENTAGE=5
TAKE_PROFIT_PERCENTAGE=10

# Database (Supabase)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=__PUBLIC_ANON_KEY__
SUPABASE_DB_PASSWORD=__DB_PASSWORD__
DATABASE_URL=postgresql://postgres:${SUPABASE_DB_PASSWORD}@db.YOUR_PROJECT_ID.supabase.co:5432/postgres

# API Keys (leave blank for paper mode)
TWITTER_BEARER_TOKEN=
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=
NEWS_API_KEY=
ALPHA_VANTAGE_KEY=
BIRDEYE_API_KEY=

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# DEX / Market Data Hosts
JUPITER_API_URL=quote-api.jup.ag
JUPITER_PRICE_API_URL=price.jup.ag
JUPITER_TOKEN_LIST_URL=token.jup.ag
METEORA_DLMM_API_URL=dlmm-api.meteora.ag
METEORA_DAMM_API_URL=dammv2-api.meteora.ag
METEORA_SEARCH_API_URL=universal-search-api.meteora.ag
METEORA_VAULT_API_URL=stake-for-fee-api.meteora.ag
ORCA_API_URL=api.orca.so
ORCA_WHIRLPOOL_URL=api.orca.so/v1/whirlpool
DEXSCREENER_API_URL=api.dexscreener.com

# Server Configuration
DASHBOARD_PORT=3000
API_PORT=8080
LOG_LEVEL=info
TRADING_MODE=paper

# Notes:
#  - Keep TRADING_MODE=paper until fully configured.
#  - Set these vars in Netlify/hosting environment for production.
#  - Do not paste real secrets into this file before committing.
